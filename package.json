{
  "name": "dreamlust-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run start-server.ts",
    "start": "NODE_ENV=production bun run src/server.ts",
    "test:redis": "bun run test-redis.js",
    "start:script": "powershell -ExecutionPolicy Bypass -File ./start-server.ps1",
    "start:monitor": "powershell -ExecutionPolicy Bypass -File ./keep-server-running.ps1",
    "build": "bun run db:generate",
    "typecheck": "bun --bun tsc --noEmit || echo 'TypeScript check skipped (no tsconfig.json)'",
    "workers": "bun run src/workers.ts",
    "worker:download": "bun run src/workers.ts",
    "dev:all": "bun --bun run src/server.ts & bun --bun run src/workers.ts",
    "dev:server": "bun run src/server.ts",
    "dev:workers": "bun run src/workers.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "bun run prisma/seed.ts",
    "db:test": "bun run scripts/test-db-connection.ts",
    "test:endpoint": "bun run scripts/test-creators-endpoint.ts",
    "seed:creators": "bun run scripts/seed-creators.ts",
    "generate:vapid": "node scripts/generate-vapid-keys.js",
    "test:load:search": "k6 run tests/load/search-load.js",
    "test:load:api": "k6 run tests/load/api-load.js",
    "test:load:auth": "k6 run tests/load/auth-load.js"
  },
  "dependencies": {
    "@aws-sdk/client-rekognition": "^3.700.0",
    "@aws-sdk/client-s3": "^3.954.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "@google-cloud/vision": "^4.0.0",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@prisma/client": "^6.19.1",
    "@react-email/components": "^1.0.1",
    "@react-email/render": "^2.0.0",
    "@sentry/node": "^10.30.0",
    "@types/qrcode": "^1.5.6",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.66.1",
    "compression": "^1.8.1",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "isomorphic-dompurify": "^2.11.2",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.11",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "qrcode": "^1.5.4",
    "razorpay": "^2.9.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "stripe": "^20.0.0",
    "ua-parser-js": "^2.0.7",
    "uuid": "^11.0.3",
    "web-push": "^3.6.7",
    "winston": "^3.19.0",
