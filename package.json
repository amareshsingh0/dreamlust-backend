{
  "name": "dreamlust-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run src/server.ts",
    "start": "NODE_ENV=production bun run src/server.ts",
    "start:script": "powershell -ExecutionPolicy Bypass -File ./start-server.ps1",
    "start:monitor": "powershell -ExecutionPolicy Bypass -File ./keep-server-running.ps1",
    "workers": "bun run src/workers.ts",
    "dev:all": "bun --bun run src/server.ts & bun --bun run src/workers.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "bun run prisma/seed.ts",
    "seed:creators": "bun run scripts/seed-creators.ts"
  },
  "dependencies": {
    "@aws-sdk/client-rekognition": "^3.700.0",
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "@google-cloud/vision": "^4.0.0",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@prisma/client": "^6.19.1",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.66.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "isomorphic-dompurify": "^2.11.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.11",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "razorpay": "^2.9.6",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "stripe": "^20.0.0",
    "uuid": "^11.0.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/csrf": "^3.1.0",
    "@types/express": "^5.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/nodemailer": "^7.0.4",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-twitter": "^1.0.40",
    "bun-types": "latest",
    "prisma": "^6.19.1",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
